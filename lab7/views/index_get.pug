p.w3-border.w3-border-black.w3-round.w3-margin.w3-padding.w3-left(name='dostepne' style='width: 20%;')
  a Dostępne: 
  br
  br
  each set in cars
    br
    img(src=set.src alt='Fiat Tipo' style='width:20%')
    a #{set.brand} 
    a Ilość: #{set.amount} 
    a Koszt wypożyczenia: #{set.rent_cost}
    a Koszt kupna: #{set.sell_cost}
    br

form.w3-margin(method='POST' action='/client')
  textarea#commandarea(name='command' rows='4' cols='50' placeholder='Enter the command (CLIENT <rent, retur>   <car>    <name>   <surname>    <rent_date>    <return_date>)')
  br
  input(type='submit')

form.w3-margin(method='POST' action='/admin')
  textarea#commandarea(name='command' rows='4' cols='50' placeholder='Enter the command (ADMIN <sell, add>   <car>    <name>   <surname>    <rent_date>    <return_date>)')
  br
  input(type='submit')

p.w3-border.w3-border-black.w3-round.w3-margin.w3-padding.w3-left(name='rezultat_komendy' style='width: 20%;')
  a Rezultat komendy: 
  br
  a #{message}

p.w3-border.w3-border-black.w3-round.w3-margin.w3-padding.w3-left(name='kupione' style='width: 20%;')
  a Kupione: 
  br
  br
  each set in bought
    br
    a #{set.name} 
    a #{set.surname} 
    a #{set.brand} 
    a #{set.cost} 
    a #{set.bought_date} 
    br




p.w3-border.w3-border-black.w3-round.w3-margin.w3-padding.w3-left(name='wypożyczone' style='width: 20%;')
  a Wypo&zdot;yczone:
  br
  br
  each set in rented
    br
    a #{set.name} 
    a #{set.surname} 
    a #{set.brand} 
    a #{set.cost} 
    a #{set.dataw} 
    a #{set.dataz} 
    a #{set.bought_date}
    br 

footer.w3-border.w3-border-light-green.w3-light-green.w3-bottom
  a.w3-text-white.w3-margin
